{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": {
      "options": "never"
    },
    "quotemark": {
      "options": "single"
    },
    "object-literal-sort-keys": false,
    "no-empty": false,
    "trailing-comma": false,
    "max-line-length": false,
    "arrow-parens": false,
    "no-restricted-globals": false
  },
  "rulesDirectory": []
}
